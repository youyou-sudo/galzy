{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build --turbopack && cp -r .next/static .next/standalone/apps/web/.next/static && cp -r public .next/standalone/apps/web/public",
    "build:webpack": "next build --webpack && cp -r .next/static .next/standalone/apps/web/.next/static && cp -r public .next/standalone/apps/web/public",
    "build:win": "next build --turbopack && bun scripts/buildWinFileCopy.ts",
    "start": "next start",
    "lint": "biome check --write",
    "docker": "cd ../.. && docker build -t mono-web:$(node -e \"console.log(require('./apps/api/package.json').version)\") -f ./apps/web/Dockerfile .",
    "docker:web": "cd ../.. && docker build -t mono-web:$(node -e \"console.log(require('./apps/api/package.json').version)\") -f ./apps/web/Dockerfile ."
  },
  "dependencies": {
    "@bprogress/next": "^3.2.12",
    "@hookform/resolvers": "^5.2.2",
    "@next/env": "^16.0.10",
    "@tanstack/react-query": "^5.90.12",
    "better-auth": "^1.4.6",
    "js.foresight": "^3.3.4",
    "lucide-react": "^0.556.0",
    "next": "^16.0.10",
    "next-runtime-env": "^3.3.0",
    "next-themes": "^0.4.6",
    "nuqs": "^2.8.5",
    "postcss": "8.5.6",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.68.0",
    "react-intersection-observer": "^10.0.0",
    "react-markdown": "^10.1.0",
    "recharts": "2.15.4",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "unist-util-visit": "^5.0.0",
    "zod": "^4.1.13",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@next/mdx": "^16.0.10",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "24.10.1",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "config": "workspace:*",
    "tw-animate-css": "^1.4.0",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@tailwindcss/oxide",
    "esbuild",
    "msgpackr-extract",
    "sharp",
    "unrs-resolver"
  ]
}
